{
  "apiVersion": "dashboard.grafana.app/v1beta1",
  "kind": "Dashboard",
  "metadata": {
    "name": "CloudCost Exporter Operations Dashboard"
  },
  "spec": {
    "uid": "1a9c0de366458599246184cf0ae8b468",
    "title": "CloudCost Exporter Operations Dashboard",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "fiscalYearStartMonth": 0,
    "refresh": "30s",
    "schemaVersion": 41,
    "panels": [
      {
        "type": "row",
        "collapsed": false,
        "title": "Overview",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "id": 0,
        "panels": []
      },
      {
        "type": "stat",
        "targets": [
          {
            "expr": "max by (provider, collector) (cloudcost_exporter_collector_last_scrape_error == 0)",
            "instant": true,
            "range": false,
            "legendFormat": "{{provider}}:{{collector}}"
          }
        ],
        "title": "Collector Status",
        "description": "Display the status of all the collectors running.",
        "transparent": false,
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 1
        },
        "repeatDirection": "h",
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "textMode": "auto",
          "wideLayout": true,
          "showPercentChange": false,
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "percentChangeColorMode": "standard",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "Up",
                    "index": 0
                  }
                }
              },
              {
                "type": "special",
                "options": {
                  "match": "null+nan",
                  "result": {
                    "text": "Down",
                    "color": "red",
                    "index": 1
                  }
                }
              }
            ]
          },
          "overrides": []
        }
      },
      {
        "type": "table",
        "targets": [
          {
            "expr": "count by (cluster, image_version) (label_replace(kube_pod_container_info{container=\"cloudcost-exporter\"}, \"image_version\", \"$2\", \"image\", \"(.*:)(.*)\"))",
            "instant": true,
            "range": false,
            "refId": "A"
          }
        ],
        "title": "Image Versions by Cluster",
        "description": "Shows which cloudcost-exporter image version is deployed on each cluster",
        "transparent": false,
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 1
        },
        "repeatDirection": "h",
        "transformations": [
          {
            "id": "seriesToRows",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "Metric": 0,
                "Value": 1
              },
              "renameByName": {
                "Value": "Pod Count"
              }
            }
          },
          {
            "id": "extractFields",
            "options": {
              "format": "kvp",
              "keepTime": false,
              "replace": false,
              "source": "Metric"
            }
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Metric": true
              },
              "indexByName": {
                "Pod Count": 2,
                "cluster": 0,
                "image_version": 1
              },
              "renameByName": {
                "Pod Count": "Pod Count",
                "cluster": "Cluster",
                "image_version": "Image Version"
              }
            }
          }
        ],
        "options": {
          "frameIndex": 0,
          "showHeader": true,
          "showTypeIcons": false,
          "footer": {
            "show": false,
            "reducer": null,
            "countRows": false
          },
          "cellHeight": "sm"
        },
        "fieldConfig": {
          "defaults": {},
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Pod Count"
              },
              "properties": [
                {
                  "id": "custom.align",
                  "value": "center"
                },
                {
                  "id": "custom.width",
                  "value": 120
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Cluster"
              },
              "properties": [
                {
                  "id": "custom.width",
                  "value": 300
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Image Version"
              },
              "properties": [
                {
                  "id": "custom.width",
                  "value": 150
                }
              ]
            }
          ]
        }
      },
      {
        "type": "timeseries",
        "targets": [
          {
            "expr": "cloudcost_exporter_collector_last_scrape_duration_seconds",
            "instant": false,
            "range": true,
            "legendFormat": "{{provider}}:{{collector}}"
          }
        ],
        "title": "Collector Scrape Duration",
        "description": "Duration of scrapes by provider and collector",
        "transparent": false,
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 7
        },
        "repeatDirection": "h",
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true,
            "sortBy": "Last *",
            "sortDesc": true,
            "calcs": [
              "lastNotNull",
              "min",
              "max"
            ]
          },
          "tooltip": {
            "mode": "",
            "sort": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 60,
                  "color": "red"
                }
              ]
            },
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "thresholdsStyle": {
                "mode": "line"
              }
            }
          },
          "overrides": []
        }
      },
      {
        "type": "row",
        "collapsed": false,
        "title": "AWS",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 15
        },
        "id": 0,
        "panels": []
      },
      {
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (cluster) (increase(cloudcost_exporter_aws_s3_cost_api_requests_total{cluster=~\"$cluster\"}[5m]))",
            "instant": false,
            "range": true,
            "legendFormat": "__auto"
          }
        ],
        "title": "CostExplorer API Requests",
        "transparent": false,
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "repeatDirection": "h",
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          },
          "overrides": []
        }
      },
      {
        "type": "timeseries",
        "targets": [
          {
            "expr": "max by (cluster) (cloudcost_exporter_aws_s3_next_scrape{cluster=~\"$cluster\"}) - time() ",
            "instant": false,
            "range": true,
            "legendFormat": "__auto"
          }
        ],
        "title": "Next pricing map refresh",
        "description": "The AWS s3 module uses cost data pulled from Cost Explorer, which costs $0.01 per API call. The cost metrics are refreshed every hour, so if this value goes below 0, it indicates a problem with refreshing the pricing map and thus needs investigation.",
        "transparent": false,
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "repeatDirection": "h",
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          },
          "overrides": []
        }
      },
      {
        "type": "row",
        "collapsed": false,
        "title": "GCP",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 22
        },
        "id": 0,
        "panels": []
      },
      {
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (cluster, status) (increase(cloudcost_exporter_gcp_gcs_bucket_list_status_total[5m]))",
            "instant": false,
            "range": true,
            "legendFormat": "{{cluster}}:{{status}}"
          }
        ],
        "title": "GCS List Buckets Requests Per Second",
        "description": "The number of requests per second to list buckets in GCS.",
        "transparent": false,
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 23
        },
        "repeatDirection": "h",
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          },
          "overrides": []
        }
      },
      {
        "type": "timeseries",
        "targets": [
          {
            "expr": "max by (cluster) (cloudcost_exporter_gcp_gcs_next_scrape{cluster=~\"$cluster\"}) - time() ",
            "instant": false,
            "range": true,
            "legendFormat": "__auto"
          }
        ],
        "title": "GCS Pricing Map Refresh Time",
        "description": "The amount of time before the next refresh of the GCS pricing map. ",
        "transparent": false,
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 23
        },
        "repeatDirection": "h",
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          },
          "overrides": []
        }
      }
    ],
    "templating": {
      "list": [
        {
          "type": "datasource",
          "name": "datasource",
          "label": "Data Source",
          "skipUrlSync": false,
          "query": "prometheus",
          "multi": false,
          "allowCustomValue": true,
          "includeAll": false,
          "auto": false,
          "auto_min": "10s",
          "auto_count": 30
        },
        {
          "type": "query",
          "name": "cluster",
          "skipUrlSync": false,
          "query": "label_values(tanka_environment_info{app=\"cloudcost-exporter\"},exported_cluster)",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "multi": true,
          "allowCustomValue": true,
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "auto": false,
          "auto_min": "10s",
          "auto_count": 30
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Annotations \u0026 Alerts",
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "type": "dashboard",
          "builtIn": 1
        }
      ]
    }
  }
}